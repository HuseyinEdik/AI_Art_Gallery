{
  "ApiSettings": {
    "BaseUrl": "http://localhost:8080/api",
    "Timeout": 300
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "AI_Art_Gallery.Services.SpringApiClient": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 52428800,
      "RequestHeadersTimeout": "00:05:00",
      "MaxRequestLineSize": 16384,
      "MaxRequestHeadersTotalSize": 65536
    }
  }
}
