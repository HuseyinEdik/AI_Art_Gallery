# Environment variables for docker-compose

# PostgreSQL
POSTGRES_DB=ai_gallery_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-secure-password-here

# Spring API
SPRING_API_IMAGE=your-dockerhub-username/ai-gallery-spring-api:latest

# MVC App
MVC_APP_IMAGE=your-dockerhub-username/ai-gallery-mvc:latest
MVC_PORT=5000

# JWT Secret (Spring tarafýnda kullanýlacak)
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRATION=86400000
