{
  "ApiSettings": {
    "BaseUrl": "http://localhost:8080/api",
    "Timeout": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "AI_Art_Gallery": "Debug"
    }
  },
  "Session": {
    "IdleTimeoutMinutes": 30
  },
  "Authentication": {
    "CookieExpireDays": 7,
    "SlidingExpiration": true
  }
}
